<div class="conteiner m-5">
  <div class="row mt-4">
    <div class="col-9">
        <h4 class="text-center">番組一覧</h4>
        <div class="mt-3">
          <% @programs.each do |program|%>
          <h4>番組情報</h4>
          <p>番組名：<%= program[:title] %></p><br>
          <p>出演者情報：<%= program[:cast] %></p><br>
          <p>放送時間：<%= program[:date] %></p><br>
          <p>局：<%= program[:channel] %></p><br>
          <% end %>
        </div>
    </div>
    <div class="col-3">
        <p>検索フォーム</p>
        <%= render 'admins/searches/search' %>
    </div>
  </div>
</div>