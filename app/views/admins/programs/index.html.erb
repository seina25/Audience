<div class="conteiner m-5">
  <div class="row mt-4">
    <div class="col-9">
        <h4 class="text-center">番組一覧</h4>
        <div class="mt-3">
          <% @programs.each do |program|%>
          <%= program.title %>
          <%= program.cast %>
          <%= program.date %>
          <%= program.channel %>
          <% end %>
        </div>
    </div>
    <div class="col-3">
        <p>検索フォーム</p>
        <%= render 'admins/searches/search' %>
    </div>
  </div>
</div>